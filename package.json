{
  "name": "datasource-landing",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build-css": "node-sass-chokidar src/styles -o src/styles/css",
    "watch-css": "npm run build-css && node-sass-chokidar public/styles -o public/styles/css --watch --recursive",
    "start-js": "nodemon ./bin/www",
    "start": "npm-run-all -p watch-css start-js"
  },
  "dependencies": {
    "body-parser": "~1.18.2",
    "cookie-parser": "~1.4.3",
    "cssnano": "^3.10.0",
    "debug": "~2.6.9",
    "express": "~4.15.5",
    "gulp": "^3.9.1",
    "gulp-imagemin": "^4.1.0",
    "gulp-newer": "^1.4.0",
    "gulp-postcss": "^7.0.1",
    "gulp-uglify": "^3.0.0",
    "log": "^1.4.0",
    "morgan": "~1.9.0",
    "pug": "2.0.0-beta11",
    "serve-favicon": "~2.4.5",
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3"
  }
}
