{
  "name": "nodejs-landing-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build-css": "node-sass-chokidar src/styles -o src/styles/css",
    "watch-css": "npm run build-css && node-sass-chokidar public/styles -o public/styles/css --watch --recursive",
    "start-js": "nodemon ./bin/www",
    "start": "npm-run-all -p watch-css start-js"
  },
  "dependencies": {
    "body-parser": "1.18.3",
    "cookie-parser": "1.4.3",
    "debug": "4.1.0",
    "express": "4.16.4",
    "log": "1.4.0",
    "morgan": "1.9.1",
    "pug": "2.0.3",
    "serve-favicon": "2.5.0"
  },
  "devDependencies": {
    "cssnano": "4.1.7",
    "gulp": "3.9.1",
    "gulp-imagemin": "4.1.0",
    "gulp-newer": "1.4.0",
    "gulp-postcss": "8.0.0",
    "gulp-uglify": "3.0.1",
    "node-sass-chokidar": "1.3.4",
    "npm-run-all": "4.1.3"
  }
}